---
// src/pages/index.astro

import BaseLayout from "../layouts/BaseLayout.astro";
import Card from "../components/Card.astro";
import * as aboutMe from '../components/aboutMe.md';
---

<BaseLayout title="Carte Guillaume Sachet" description="Carte de visite de Guillaume Sachet Data Scientist">
	<Card
	  name="Guillaume Sachet"
	  position="Data Scientist - ML Engineer"
	  aboutMe={aboutMe.compiledContent()}
	  linkedin="https://www.linkedin.com/in/guillaume-sachet/"
	  malt="https://www.malt.fr/profile/guillaumesachet"
	  github="https://github.com/GuillaumeSachet"
	  cvLink="/files/CV_Guillaume_Sachet.pdf"
	  profileImage="/images/profile.jpg"
	/>
</BaseLayout>